{
  "scripts": {
    "build2": "tsc",
    "dev:server": "ts-node-dev -r tsconfig-paths/register  --transpile-only --ignore-watch node_modules src/index.ts",
    "dev:debug": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/index.ts",
    "start2": "ts-node dist/index.ts",
    "start": "node dist/index.js",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files"
  },
  "dependencies": {
    "@types/multer": "^1.4.5",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "gm": "^1.23.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.9",
    "multer": "^1.4.2",
    "path": "^0.12.7",
    "ts-node-dev": "^1.1.6",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.12",
    "@types/gm": "^1.18.9",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/mongodb": "^4.0.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.2"
  }
