{
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./",
      "strict": true,
      "strictPropertyInitialization": false,
      "baseUrl": "./",
      "allowJs": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "exclude": ["node_modules"],
    "ts-node": {
      "compilerOptions": {
        "module": "commonjs"
      }
    },
  }
  